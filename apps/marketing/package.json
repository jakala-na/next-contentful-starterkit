{
  "name": "marketing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "check-types": "tsc --noEmit",
    "generate:schema": "source .env.local && gql.tada generate-schema --header \"Authorization: Bearer $CONTENTFUL_DELIVERY_API\" \"https://graphql.contentful.com/content/v1/spaces/$CONTENTFUL_SPACE\"",
    "generate:output": "gql.tada generate-output && gql.tada turbo",
    "generate:component": "pnpm node --env-file=.env.local cli/generate-component.mjs",
    "generate:ctf-seed": "contentful space export --export-dir=migrations/ --content-file=ctf-seed.json --use-verbose-renderer=true --download-assets --skip-webhooks",
    "seed": "contentful space import --content-file ./migrations/ctf-seed.json"
  },
  "dependencies": {
    "@contentful/content-source-maps": "^0.11.5",
    "@contentful/live-preview": "^4.5.13",
    "@contentful/rich-text-react-renderer": "^15.17.1",
    "@contentful/rich-text-types": "^16.8.5",
    "@inquirer/prompts": "^7.0.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@repo/tailwind-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@urql/core": "^5.0.8",
    "@urql/exchange-persisted": "^4.2.0",
    "@vercel/toolbar": "^0.1.22",
    "analytics": "^0.8.11",
    "encoding": "^0.1.13",
    "gql.tada": "^1.8.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.368.0",
    "next": "14.2.7",
    "next-international": "^1.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.8.1",
    "server-only": "^0.0.1",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.4.3",
    "urql": "^4.0.7",
    "use-analytics": "^1.1.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.4",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/webpack-dev-server": "^3.2.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@repo/eslint-config": "workspace:*",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/lodash": "^4.17.13",
    "@types/node": "20.14.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/use-analytics": "^0.0.3",
    "autoprefixer": "^10.4.14",
    "axe-core": "^4.6.1",
    "chalk": "^5.3.0",
    "contentful": "^11.2.0",
    "cypress": "^13.16.0",
    "cypress-axe": "^1.2.0",
    "cypress-image-snapshot": "^4.0.1",
    "esbuild": "0.20.1",
    "inquirer": "9",
    "jest": "^29.7.0",
    "ora": "^8.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "prettier-plugin-curly": "^0.2.1",
    "simple-scaffold": "^2.3.2",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "[filepath].steps.{js,jsx,ts,tsx}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ],
    "html": {
      "enabled": true
    }
  }
}
