{
  "importOrder": [
    "",
    "^react$",
    "^next(-[^/]+)?(/.*)?$",
    "",
    "<TYPES>",
    "<TYPES>^[.]",
    "",
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^#/(.*)$",
    "",
    "^[./]",
    "",
    "^(?!.*[.]css$)[./].*$",
    ".css$"
  ],
  "importOrderTypeScriptVersion": "5.3.3",
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tailwindFunctions": ["cn"],
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-curly",
    "prettier-plugin-tailwindcss"
  ]
}
