{
  "name": "next-contentful-starterkit",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "generate:schema": "source .env.local && gql.tada generate-schema --header \"Authorization: Bearer $CONTENTFUL_DELIVERY_API\" \"https://graphql.contentful.com/content/v1/spaces/$CONTENTFUL_SPACE\"",
    "generate:output": "gql.tada generate-output && gql.tada turbo",
    "generate:component": "yarn node --env-file=.env.local cli/generate-component.mjs",
    "generate:ctf-seed": "contentful space export --export-dir=migrations/ --content-file=ctf-seed.json --use-verbose-renderer=true --download-assets",
    "seed": "contentful space import --content-file ./migrations/ctf-seed.json",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@contentful/content-source-maps": "^0.11.5",
    "@contentful/live-preview": "^4.5.13",
    "@contentful/rich-text-react-renderer": "^15.17.1",
    "@inquirer/prompts": "^7.0.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-slot": "^1.0.2",
    "@storybook/test": "^8.0.4",
    "@types/node": "18.11.11",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@urql/exchange-persisted": "^4.2.0",
    "@vercel/toolbar": "^0.1.22",
    "analytics": "^0.8.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "encoding": "^0.1.13",
    "gql.tada": "^1.5.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.368.0",
    "next": "14.2.7",
    "next-international": "^1.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.8.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.4.3",
    "urql": "^4.0.7",
    "use-analytics": "^1.1.0"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.7.0",
    "@badeball/cypress-cucumber-preprocessor": "^18.0.4",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@cypress/webpack-dev-server": "^3.2.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-onboarding": "^8.0.4",
    "@storybook/addon-themes": "^8.0.4",
    "@storybook/blocks": "^8.0.4",
    "@storybook/nextjs": "^8.0.4",
    "@storybook/react": "^8.0.4",
    "@types/cypress-image-snapshot": "^3.1.6",
    "@types/lodash": "^4.17.13",
    "@types/use-analytics": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^8.12.2",
    "autoprefixer": "^10.4.14",
    "axe-core": "^4.6.1",
    "chalk": "^5.3.0",
    "chromatic": "^6.21.0",
    "commitizen": "^4.3.1",
    "contentful": "^11.2.0",
    "cypress": "^12.2.0",
    "cypress-axe": "^1.2.0",
    "cypress-image-snapshot": "^4.0.1",
    "esbuild": "0.20.1",
    "eslint": "^8.48.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "ora": "^8.1.0",
    "postcss": "^8.4.27",
    "prettier": "^3.0.3",
    "prettier-plugin-curly": "^0.2.1",
    "shadcn": "^2.1.0",
    "simple-scaffold": "^2.3.2",
    "storybook": "^8.0.4",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.2",
    "webpack": "^5.75.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "[filepath].steps.{js,jsx,ts,tsx}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ],
    "html": {
      "enabled": true
    }
  },
  "resolutions": {
    "jackspeak": "2.1.1",
    "@storybook/manager-webpack5/html-webpack-plugin": "^5.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
